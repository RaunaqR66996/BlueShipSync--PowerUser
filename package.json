{
  "name": "blueshipsync-poweruser",
  "version": "1.0.0",
  "description": "A comprehensive supply-chain-native application for modern logistics and shipping operations",
  "main": "src/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon src/server/index.js",
    "client:dev": "cd src/client && npm run dev",
    "build": "npm run build:server && npm run build:client",
    "build:server": "babel src/server -d dist/server",
    "build:client": "cd src/client && npm run build",
    "start": "node dist/server/index.js",
    "test": "jest",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:performance && npm run test:security",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:performance": "npm run test:performance:load && npm run test:performance:stress",
    "test:performance:load": "artillery run tests/performance/load-test.yml",
    "test:performance:stress": "artillery run tests/performance/stress-test.yml",
    "test:security": "npm audit && snyk test",
    "test:ai-agents": "jest --testPathPattern=ai-agents",
    "test:e2e": "playwright test",
    "lint": "eslint src/ --ext .js,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,ts,tsx,json,css,md}",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "db:reset": "knex migrate:rollback --all && npm run db:migrate && npm run db:seed"
  },
  "keywords": [
    "supply-chain",
    "logistics",
    "shipping",
    "inventory-management",
    "order-management",
    "ai-integration",
    "blueshipsync"
  ],
  "author": "Shaik Raunaq Fardeen",
  "license": "PROPRIETARY",
  "repository": {
    "type": "git",
    "url": "https://github.com/RaunaqR66996/BlueShipSync--PowerUser.git"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "axios": "^1.6.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "uuid": "^9.0.1",
    "langchain": "^0.0.208",
    "openai": "^4.20.1",
    "graphql": "^16.8.1",
    "apollo-server-express": "^3.12.1",
    "knex": "^3.0.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/pg": "^8.10.9",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/lodash": "^4.14.202",
    "@types/uuid": "^9.0.7",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.14",
    "typescript": "^5.3.3",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "artillery": "^2.0.0",
    "snyk": "^1.1248.0",
    "playwright": "^1.40.1",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
