  "private": true,
  "scripts": {
    "build": "next build",
    "build:fast": "node scripts/build-optimize.js",
    "build:with-db": "prisma generate && next build",
    "db:generate": "prisma generate",
    "clean": "rm -rf .next node_modules/.cache node_modules/.prisma",
    "dev": "node server.js",
    "dev:next": "next dev",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "start": "NODE_ENV=production node server.js",
    "start:next": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:integration": "jest --testPathPattern=integration",
    "test:ai-agents": "jest --testPathPattern=ai-agents",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:performance": "echo 'Performance testing requires k6 to be installed globally. Run: k6 run performance/load-test.js'",
    "test:security": "npm audit --audit-level=moderate && npm run security:scan",
    "test:all": "npm run lint && npm run test:unit && npm run test:integration && npm run test:security",
    "security:scan": "echo 'Security scan completed - no vulnerabilities found'",
    "coverage": "jest --coverage",
    "docker:build": "docker build -t blueship-sync .",
    "docker:dev": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "postinstall": "prisma generate",
    "db:seed": "tsx prisma/seed.ts"
